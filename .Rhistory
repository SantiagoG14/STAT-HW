ggplot(data = model.aug) +
aes(x = .fitted, y = .resid) +
geom_point() +
geom_hline(yintercept = 0, color = "blue") +
ggtitle("Residuals vs. Fitted") +
xlab("Fitted Values") +
ylab("Residuals")
